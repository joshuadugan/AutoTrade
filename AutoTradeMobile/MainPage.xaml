<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AutoTradeMobile.MainPage"
             xmlns:ViewModel="clr-namespace:AutoTradeMobile"
             x:DataType="ViewModel:MainPage"
             Title="">

    <ScrollView>
        <VerticalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center"
            HorizontalOptions="Center">

            <ImageButton Command="{Binding GoCommand}" Source="etrade_logo.png" WidthRequest="300" />

        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
